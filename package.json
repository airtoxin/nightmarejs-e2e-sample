{
  "name": "nightmarejs-e2e-sample",
  "version": "0.0.1",
  "description": "E2E test sample repository using Nightmare.js",
  "main": "lib/index.js",
  "scripts": {
    "build": "webpack",
    "watch": "webpack --watch",
    "dev": "webpack-dev-server",
    "test:unit": "mocha --require jsdom-global/register --require ignore-styles --compilers jsx:babel-register --recursive test/unit",
    "test:lint": "eslint {src,test,webpack.config.babel.js} --fix",
    "test:e2e": "babel-node test/e2e/boot.js",
    "test": "npm run test:lint && npm run test:unit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/airtoxin/nightmarejs-e2e-sample"
  },
  "keywords": [],
